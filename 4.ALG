Algoritmo "4"

Var
  valor_fixado , valor_vendas : real
  resultado : caractere

Inicio

  escreva ("Digite o valor fixado: ")
  leia (valor_fixado)
  escreva ("Digite o valor de vendas de cada funcionário: ")
  leia (valor_vendas)


  
  se (valor_vendas > valor_fixado) então
  escreva ("Ganhou!")
  
  senão (valor_fixado > valor_vendas)
  escreva ("Não ganhou!")
  
  fimse
  



Fimalgoritmo